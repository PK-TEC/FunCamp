<% layout('layouts/boilerplate') %>
<h1>Register</h1>
<form action="/login" method="POST" class="form-validation" novalidate>
	<div class="mb-3">
		<label lass="form-label" for="username">Username</label>
		<input
			class="form-control"
			type="text"
			name="username"
			id="username"
			required
		/>
		<div class="valid-feedback">Good!</div>
	<div class="mb-3">
		<label lass="form-label" for="password">Password</label>
		<input
			class="form-control"
			type="password"
			name="password"
			id="password"
			required
		/>
		<div class="valid-feedback">Good!</div>
    </div>
    <div class="mb-3">
				<button class="btn btn-success">Login</button>
			</div>
</form>
